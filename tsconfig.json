{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "strict": true,
    "removeComments": false,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "skipLibCheck": true
  }
}
